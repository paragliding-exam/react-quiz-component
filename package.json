{
  "name": "@paragliding-exam/react-quiz-component",
  "version": "0.5.1",
  "description": "React Quiz Component",
  "main": "lib/Quiz.js",
  "scripts": {
    "dev": "concurrently \"npm run lib:watch\" \"npm run docs\"",
    "lib": "babel src/lib -d lib --copy-files",
    "lib:watch": "babel src/lib -w -d lib --copy-files",
    "docs": "webpack-dev-server --mode development",
    "docs:prod": "webpack --mode production",
    "lint": "eslint src --ext .jsx",
    "prepare": "husky install",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "react",
    "reactjs",
    "react-quiz-component",
    "react-quiz",
    "quiz"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "babel-loader": "^8.0.5",
    "concurrently": "^3.5.1",
    "css-loader": "^3.4.2",
    "eslint": "^8.0.0",
    "eslint-config-react-app": "^7.0.1",
    "html-webpack-plugin": "5.3.2",
    "husky": "^8.0.3",
    "prop-types": "^15.7.2",
    "react-dom": "^18.2.0",
    "style-loader": "^0.21.0",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "prettier": "^2.7.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/jsx-no-target-blank": "off"
    }
  },
  "author": "WONG, Wing Kam <wingkwong.code@gmail.com>",
  "homepage": "https://github.com/paragliding-exam/react-quiz-component",
  "repository": {
    "type": "git",
    "url": "https://github.com/paragliding-exam/react-quiz-component.git"
  },
  "dependencies": {
    "dompurify": "^2.0.8",
    "react": "^18.2.0",
    "snarkdown": "^2.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
